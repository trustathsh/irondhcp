#This is the configuration file for irondhcp

# Path to the ISC DHCP leases file to be used
irondhcp.parser.leasesfile=dhcpd.leases

# The interval in seconds to check the leases file
# for new changes.
irondhcp.parser.interval=1

# The url to the MAP server
irondhcp.server.url=https://localhost:8443/

# Whether or not to use basic authentication
irondhcp.server.basicauth.enabled=true

# Username used for basic authentication
irondhcp.server.basicauth.user=dhcp

# Password used for basic authentication
irondhcp.server.basicauth.password=dhcp

# Path to the keystore to be used by irondhcp
irondhcp.keystore.file=/keystore/irondhcp.jks

# Password of the keystore
irondhcp.keystore.password=irondhcp

# Path to the truststore to be used by irondhcp
irondhcp.truststore.file=/keystore/irondhcp.jks

# Password of the truststore
irondhcp.truststore.password=irondhcp

# The address of the DHCP server to be used in
# the ip-mac metadata.
irondhcp.dhcpd.ip=10.0.1.230

# The version of the DHCP server
irondhcp.dhcpd.version=

# Whether or not to publish self-information
irondhcp.selfpublish.enable=true

# The device name for self-publishing
irondhcp.selfpublish.device=isc-dhcp-server-device

# Whether or not to publish request-for-investigation for each new lease
irondhcp.publish.requestforinvestigation=true

# Whether or not to publish discovered-by (device<->mac-address) for each new lease
irondhcp.publish.discoveredby=true
